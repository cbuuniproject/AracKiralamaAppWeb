@model IEnumerable<BankAppWeb.Soap.KullaniciService.Kullanici>

@{
    ViewBag.Title = "Tüm Kullanıcılar";
}


<h2>@ViewBag.Title</h2>

<p>
    @Html.ActionLink("Yeni Oluştur", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.Encode("Kullanıcı Adı")
        </th>
        <th>
            @Html.Encode("Kullanıcı Turu")
        </th>
        <th></th>
    </tr>


    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.kullaniciAd)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.kullaniciTuru)
            </td>
            <td>
                @Html.ActionLink("Detaylar", "Detaylar", new { id = item.kullaniciId }) |
                @Html.ActionLink("Sil", "Delete", new { id = item.kullaniciId })
            </td>
        </tr>
    }

</table>
